{
  "name": "vidya_app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "npx react-native run-ios  --simulator='iPhone 11 Pro Max'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "commit": "git-cz",
    "web": "expo start --web"
  },
